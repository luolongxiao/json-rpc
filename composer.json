{
    "name": "point/rpc-client",
    "description": "实现thinkphp6使用jsonrpc-http访问hyperf服务.",
    "license": "poin",
    "keywords": [
        "php",
        "Point",
        "rpc",
        "rpc-client",
        "json-rpc"
    ],
    "homepage": "",
    "support": {
        "docs": "https://Point.wiki",
        "issues": "https://github.com/Point/Point/issues",
        "pull-request": "https://github.com/Point/Point/pulls",
        "source": "https://github.com/Point/Point"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitee.com/yuedao123_1/points-rpc.git"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "markrogoyski/math-php": "^2.0"
    },
    "suggest": {
        "Point/service-governance": "Required to fetch the nodes info from service governance.",
        "Point/di": "For better container experience.",
        "Point/pool": "Required to use connection pool."
    },
    "autoload": {
        "psr-4": {
            "Point\\RpcClient\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
